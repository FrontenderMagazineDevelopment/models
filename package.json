{
  "name": "@frontender-magazine/models",
  "version": "1.0.0",
  "description": "mongodb models",
  "main": "lib/index.js",
  "scripts": {
    "start": "gulp css:common",
    "release": "npm version patch && git push --tags && npm publish --access public",
    "clean": "rimraf public lib",
    "build": "npm run clean && babel source --minified -s true -d lib --ignore __tests__/**,__mocks__/** ",
    "build:doc": "documentation build src/*.js* -o public -f html",
    "test": "export NODE_ENV=test && jest --no-cache",
    "test:watch": "export NODE_ENV=test && jest --no-cache --watch",
    "test:coverage": "export NODE_ENV=test && jest --no-cache --coverage",
    "lint": "npm run format && npm-run-all lint:*",
    "lint:js": "eslint ./source/** --ext .js --color -f stylish --fix",
    "lint:doc": "documentation lint source/**/*.js*",
    "minify": "babel-minify lib/*.js -d lib --removeConsole --removeDebugger",
    "precommit": "git add . -A && lint-staged",
    "commitmsg": "commit-msg ${GIT_PARAMS}"
  },
  "lint-staged": {
    "*.{jsx,js}": [
      "eslint --color -f stylish --fix",
      "git add . -A",
      "jest --bail --findRelatedTests"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:TemplateMonster/PlasmaPlatform/Frontend/tm-amazon-rename.git"
  },
  "keywords": [
    "tm",
    "amazon",
    "bucket",
    "replacer"
  ],
  "author": "Anton Nemtsev <thesilentimp@gmail.com> (http://silentimp.info/)",
  "license": "CC-BY-4.0",
  "dependencies": {
    "@plasma-platform/tm-message-hook": "^1.0.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-minify": "^0.2.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-stage-0": "^6.24.1",
    "documentation": "^5.1.1",
    "dotenv": "^4.0.0",
    "enzyme": "^3.2.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.0.2",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "jest-enzyme": "^3.6.1",
    "lint-staged": "^5.0.0",
    "mongoose": "^4.13.5",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.8.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "restify": "^6.3.4",
    "restify-cookies": "^0.2.2",
    "rimraf": "^2.6.2"
  }
}
